{
    "name": "image-extracter",
    "version": "1.0.0",
    "description": "extracting image for provided time stamp from a given video",
    "main": "build/server.js",
    "scripts": {
        "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
        "doc": "tsoa spec --host http://localhost:8070/",
        "build": "tsoa spec-and-routes && tsc",
        "start": "node build/server.js"
    },
    "license": "MIT",
    "dependencies": {
        "body-parser": "^1.19.2",
        "express": "^4.17.3",
        "express-winston": "^4.2.0",
        "tsoa": "^3.14.1",
        "winston": "^3.6.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/node": "^17.0.19",
        "concurrently": "^7.0.0",
        "nodemon": "^2.0.15",
        "ts-node": "^10.5.0",
        "typescript": "^4.5.5"
    }
}
